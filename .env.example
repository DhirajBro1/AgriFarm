# AgriFarm Environment Variables
# Copy this file to .env.local and fill in your actual values

# Get your api from here
# https://my.plantnet.org/ 
EXPO_PUBLIC_PLANTNET_API_KEY=your_api_key

# Optional: Default coordinates for disease detection
# These will be used if location permission is not granted
EXPO_PUBLIC_DEFAULT_LATITUDE=27.7172
EXPO_PUBLIC_DEFAULT_LONGITUDE=85.3240
